    "cache-loader": "1.x",
    "comment-json": "4.x",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "0.28.x",
    "file-loader": "1.x",
    "http-server": "0.12.x",
    "i18next-parser": "^3.3.0",
    "mini-css-extract-plugin": "0.4.x",
    "react-dom": "^17.0.1",
    "read-pkg": "5.x",
    "resolve-url-loader": "2.x",
    "sass-loader": "6.x",
    "thread-loader": "1.x",
    "ts-loader": "6.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "consolePlugin": {
    "name": "clowder-plugin",
